        "controls": [
            {
                "title": {
                    "type": "panel",
                    "anchor_from": "top_right",
                    "anchor_to": "top_right",
                    "controls": [
                        {
                            "title_text": {
                                "layer": 5,
                                "type": "label",
                                "font_type": "MinecraftSeven", 
                                "text": "#title_text",
                                "text_alignment": "center",
                                "font_scale_factor": 0.5,
                                "anchor_from": "top_middle",
                                "anchor_to": "top_middle",
                                "offset": [
                                    0,  
                                    -17
                                ],
                                "shadow": false,
                                "bindings": [
                                    {
                                        "binding_name": "#title_text",
                                        "binding_type": "global"
                                    }
                                ]
                            }
                        }, 
                        {
                            "close_button@server_form.zeqa_close_button": {
                                "anchor_from": "top_right",
                                "anchor_to": "top_right",
                                "offset": [
                                    3,
                                    -3
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "contents": {
                    "size": [
                        "100%",
                        "100%"
                    ],
                    "layer": 2,
                    "type": "image",
                    "texture": "zeqa/textures/ui/zeqaui/background",
                    "keep_ratio": false,
                    "clips_children": true,
                    "allow_clipping": true,
                    "controls": [
                        {
                            "add_panel": {
                                "type": "stack_panel",
                                "orientation": "horizontal",
                                "offset": [
                                    -2,
                                    0
                                ],
                                "anchor_to": "center",
                                "anchor_from": "center",
                                "size": [
                                    "100% -30px",
                                    "100%"
                                ],
                                "controls": [
                                    {
                                        "zeqa_scroll_panel@zeqa_scroll_panel": {}
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    "zeqa_scroll_panel": {
        "type": "image",
