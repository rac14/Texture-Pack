{
    "namespace": "server_form",
    "third_party_server_screen@common.base_screen": {
        "$screen_content": "server_form.zeqa_main_content",
        "button_mappings": [
            {
                "from_button_id": "button.menu_cancel",
                "to_button_id": "button.menu_exit",
                "mapping_type": "global"
            }
        ]
    },
    "zeqa_main_content": {
        "type": "panel",
        "size": [
            "100%",
            "100%"
        ],
        "controls": [
            {
                "server_form_factory": {
                    "type": "factory",
                    "control_ids": { 
                        "long_form": "@server_form.zeqas_long_form",
                        "custom_form": "@server_form.custom_form"
                    }
                }
            }
        ]
    },
    "zeqas_long_form": {
        "type": "panel",
        "$ffa": "               s",
        "$rank": "ranks",
        "$rules": "rules",
        "size": [
            "100%",
            "100%"
        ],
        "controls": [
            {
                "long_form@long_form": {
                    "bindings": [
                        {
                            "binding_type": "global",
                            "binding_condition": "none",
                            "binding_name": "#title_text",
                            "binding_name_override": "#title_text"
                        },
                        {
                            "source_property_name": "(((#title_text - $ffa)  = #title_text) and ((#title_text - $rank) = #title_text)  and ((#title_text - $rules) = #title_text))",
                            "binding_type": "view",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "zeqa_long_form@zeqa_long_form": {
                    "bindings": [
                        {
                            "binding_type": "global",
                            "binding_condition": "none",
                            "binding_name": "#title_text",
                            "binding_name_override": "#title_text"
                        },
                        {
                            "source_property_name": "(not((#title_text - $ffa) = #title_text))",
                            "binding_type": "view",
                            "target_property_name": "#visible"
                        }
                    ]
                } 
            },
            {
                "zeqa_rank_form@zeqa_rank_form": {
                    "bindings": [
                        {
                            "binding_type": "global",
                            "binding_condition": "none",
                            "binding_name": "#title_text",
                            "binding_name_override": "#title_text"
                        },
                        {
                            "source_property_name": "(not((#title_text - $rank) = #title_text))",
                            "binding_type": "view",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "zeqa_rules_form@zeqa_rules_form": {
                    "bindings": [
                        {
                            "binding_type": "global",
                            "binding_condition": "none",
                            "binding_name": "#title_text",
                            "binding_name_override": "#title_text"
                        },
                        {
                            "source_property_name": "(not((#title_text - $rules) = #title_text))",
                            "binding_type": "view",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            }
        ]
    },
    "zeqa_long_form": {
        "type": "panel",
        "anchor_from": "center",
        "anchor_to": "center",
        "size": [
            "100% - 50px",
            "100% - 50px"
        ],
        "offset": [
            0,
            0
        ],
